@using Blog
@{
    Author Author = Model.Author;
}
<!DOCTYPE html>

<html lang="zh-cn">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>@Model.Title</title>

    <link rel="stylesheet" href="http://cdn.bootcss.com/bootstrap/3.3.4/css/bootstrap.min.css">
    <link rel="stylesheet" href="http://cdn.bootcss.com/bootstrap/3.3.4/css/bootstrap-theme.min.css">

    @*<link href="~/css/bootstrap.min.css" rel="stylesheet" />
        <link href="~/css/bootstrap-theme.min.css" rel="stylesheet" />*@

    <link href="~/css/blog.css" rel="stylesheet" />

    <style>
        .footer {
            padding: 40px 0;
            color: #999;
            text-align: left;
            background-color: #f9f9f9;
        }
    </style>
</head>
<body>
    <div class="container">
        @*@RenderSection("blogHeader", false)*@
        <div class="row">
            <div class="col-sm-10">
                @RenderBody()
            </div>

            <div class="col-sm-2">
                <div class="panel">
                    <div class="text-center">
                        <img class="img-circle" data-filename="photo.jpg" style="width: 160px;"
                             src="data:image/jpeg;base64,">

                        <h3>
                            @Model.Author.NickName
                        </h3>
                    </div>
                    <p>
                        @Author.Resume
                    </p>
                    <br/>
                    <address>
                        <strong>联系方式</strong><br>
                        <span class="glyphicon glyphicon-user" aria-hidden="true">&nbsp;</span><a href="http://weibo.com/crazer">新浪微博</a><br />
                        <span class="glyphicon glyphicon-envelope" aria-hidden="true">&nbsp;</span><a href="mailto:#">crazor@foxmail.com</a><br />
                    </address>

                </div>
                <div class="panel">
                    <a class="btn btn-primary btn-sm" href="/">首页</a>
                </div>
                <div class="panel">
                    <strong>分类</strong>
                    <div class="row">
                        @foreach (Info info in Model.Categories)
                        {
                            <div class="col-sm-10">
                                <a class="btn btn-primary btn-xs" href="/category/@info.Key" style="margin-top:4px">@info.Key <span class="badge">@info.Count</span></a>
                            </div>

                        }
                    </div>
                </div>

                <div class="panel">
                    <strong>归档</strong>
                    <div class="row">
                        @foreach (Info info in Model.Archives)
                        {
                            <div class="col-sm-10">
                                <a class="btn btn-primary btn-xs" href="/archive/@info.Key" style="margin-top:4px">@info.Key <span class="badge">@info.Count</span></a>
                            </div>

                        }
                    </div>
                </div>


                <div class="footer">
                    Power by Roy © 2015<br />
                    <a href="/admin" title="小暗门"><span class="glyphicon glyphicon-road" aria-hidden="true"></span></a>
                    <br/>
                    <script type="text/javascript">var cnzz_protocol = (("https:" == document.location.protocol) ? " https://" : " http://"); document.write(unescape("%3Cspan id='cnzz_stat_icon_1255281075'%3E%3C/span%3E%3Cscript src='" + cnzz_protocol + "s11.cnzz.com/z_stat.php%3Fid%3D1255281075%26show%3Dpic' type='text/javascript'%3E%3C/script%3E"));</script>
                </div>
            </div>
        </div>
    </div>

    <script src="http://cdn.bootcss.com/jquery/1.11.2/jquery.min.js"></script>
    <script src="http://cdn.bootcss.com/bootstrap/3.3.4/js/bootstrap.min.js"></script>
</body>
</html>
